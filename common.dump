    echo "Dumping to tape..."
    opts="$_dump_debug$dump_args -l0 -f $dumptape -M $media_label -L $session_label $SCRATCH_MNT"
    echo "xfsdump $opts" | _dir_filter  
    xfsdump $opts 2>&1 | tee -a $seq.full | _dump_filter
}


